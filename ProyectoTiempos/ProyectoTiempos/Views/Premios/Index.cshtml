@model IEnumerable<ProyectoTiempos.Models.Premio>

@{
    ViewBag.Title = "Index";
}

<h3>Premios</h3>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Multiveces)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Multiveces)
        </td>
        
    </tr>
}

</table>
